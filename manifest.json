{
  "name": "Loventis Logout Plugin",
  "description": "This extension will log the current user out of a loventis session",
  "version": "1.0",

  "manifest_version": 2,

  "browser_action": {
   "default_icon": "icon.png",
   "default_title": "Love the Logout"
  },

  "background": {
    "scripts": ["eventPage.js"]
  },

  "permissions": [
   "activeTab",
   "tabs",
   "http://*/",
   "https://*/"
   ]
}
